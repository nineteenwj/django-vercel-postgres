{
    "builds": [
      {
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "runtime": "python3.10"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build"
      }    
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "core/wsgi.py"
      }
    ]
  }
